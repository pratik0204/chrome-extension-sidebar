{
  "manifest_version": 2,
  "name": "Teesco",
  "version": "1.0",
  "description": "teesco",
  "icons": {
    "128": "128.png",
    "32": "32.png",
    "16": "16.png"
  },
  "browser_action": {
    "default_icon": "16.png"
  },
  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": ["teesco.html"],
  "permissions": ["tabs"]
}
